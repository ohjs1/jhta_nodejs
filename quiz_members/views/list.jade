extends layout
block content
  h1  멤버목록
  table(border='1', width='700')
    tr
      th  멤버번호
      th  이름
      th  폰번호
      th  주소
      th  가입일
      th  삭제
      th  수정
    each user in list
      tr
        td  #{user.num}
        td  #{user.name}
        td  #{user.phone}
        td  #{user.addr}
        td  #{user.regdate}
        td
          a(href='/users/delete?num=#{user.num}')  삭제
        td
          a(href='/users/update?num=#{user.num}')  수정
    p
      a(href='/')  메인페이지